  "version": "4.0.0",
  "scripts": {
    "dev": "yarn run start",
    "start": "expo start",
    "simulator:install:android": "yarn expo run:android --device",
    "simulator:install:ios": "yarn expo run:ios --device",
    "prebuild": "expo prebuild --clean",
    "prebuild:remove": "rm -rf ios/ android/",
    "android": "DARK_MODE=media expo start --android",
    "doctor": "npx expo-doctor",
    "build:local:acceptance:android": "eas build --platform android --profile acceptance --local",
    "build:local:acceptance:ios": "eas build --platform ios --profile acceptance --local",
    "build:local:production:android": "eas build --platform android --profile production --local",
    "build:local:production:ios": "eas build --platform ios --profile production --local",
    "build:aes:acceptance:android": "eas build --platform android --profile acceptance",
    "build:aes:acceptance:ios": "eas build --platform ios --profile acceptance",
    "build:aes:production:android": "eas build --platform android --profile production",
    "build:aes:production:ios": "eas build --platform ios --profile production",
    "ios": "DARK_MODE=media expo start --ios",
    "web": "DARK_MODE=media expo start --web",
    "lint": "expo lint"
